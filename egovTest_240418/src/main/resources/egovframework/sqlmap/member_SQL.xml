<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap>
<typeAlias  alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
<typeAlias  alias="memberVO" type="member.service.MemberVO"/>
	<!-- 회원가입 -->
	<insert id="memberDAO.insertMember">
		INSERT INTO MEMBER(USERID, PASS, NAME, GENDER, BIRTH, TEL, ADDRESS)
			VALUES(#userid#,#pass#,#name#,#gender#,#birth#,#tel#,#address#)
	</insert>
	
	<!-- 아이디 중복 체크 -->
	<select id="memberDAO.idChk">
		SELECT *
		FROM MEMBER
		WHERE USERID = #userid#
	</select>

</sqlMap>